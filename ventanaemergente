<script type="text/javascript">
var tagContext = "falabella-co";
var url = window.location.href;
//Condicional Global de Ejecucion
if ((new RegExp("\/" + tagContext + "\/$")).test(url) ||
	(new RegExp("\/" + tagContext + "\/index.jsp")).test(url) ||
	(new RegExp("\/" + tagContext + "\/category\/")).test(url) ||
	(new RegExp("\/" + tagContext + "\/search\/")).test(url) ||
	(new RegExp("\/" + tagContext + "\/product\/")).test(url) ||
	(new RegExp("\/" + tagContext + "\/cart\/cart.jsp")).test(url) ||
	(new RegExp("\/" + tagContext + 
"\/checkout\/submitOrderSuccess.jsp")).test(url)
	){
		local_criteo_array = [];
		local_criteo_array.push(
			{event: "setAccount", account: 13929},
			{event: "setCustomerId", id: _satellite.getVar("dyn_user_id")},
			{event: "setSiteType", type: "d"}
		);
		if ((new RegExp("\/" + tagContext + "\/$")).test(url) || (new 
RegExp("\/" + tagContext + "\/index.jsp")).test(url)){
			//Home
			local_criteo_array.push(
				{event: "viewHome"}
			);
		} else if ((new RegExp("\/" + tagContext + "\/category\/")).test(url) 
|| (new RegExp("\/" + tagContext + "\/search\/")).test(url)) {
			//Category or PLP
			items = [];
			$(".detalle > a:eq(0), .detalle > a:eq(1), .detalle > 
a:eq(2)").each(function(key, element){
				items.push(($(element).attr("href").split("/"))[3]);
			});
			local_criteo_array.push(
				{event: "viewList", item: items}
			);
		} else if ((new RegExp("\/" + tagContext + "\/product\/")).test(url)) {
			//PDP
			local_criteo_array.push(
				{event: "viewItem", item: productId}
			);
		} else if ((new RegExp("\/" + tagContext + 
"\/cart\/cart.jsp")).test(url)) {
			//Cart
			items = [];
			 $(".omsProductoBolsa").each(function(key, element){
                               items.push({
                                      id : ($(element).find(".detalle > 
a").attr("href").split("/"))[3],
                                      price: 
parseFloat($(element).find(".omsPreciosBolsa 
.unitPriceD").text().trim().replace("\$", "").replace(/\./g, "")),
                                      quantity: 
parseInt($.trim($(element).find("select").val()))
                               });
                       });
			local_criteo_array.push(
				{event: "viewBasket", item: items}
			);
		} else if((new RegExp("\/" + tagContext + 
"\/checkout\/submitOrderSuccess.jsp")).test(url)){
		
			lines = s.products.split(";");
			if (lines.length > 3){
				lines.splice(lines.length - 4, 1);
			}
			line_items = [];
			for(index = 0; index < lines.length; index = index + 3)
			{
				   item = {};
				   item.id = lines[index];
				   item.quantity = lines[index + 1];
				   item.price = Number(lines[index + 2].replace(",",""));
				   line_items.push(item);
			}
			local_criteo_array.push( 
						{ event: "setSiteType", type: "d" },
						{ event: "trackTransaction" , id: s.purchaseID,
							deduplication: '1', item: line_items
			});
		}
    //    	console.log(local_criteo_array);
		window.criteo_q = window.criteo_q || [];
    	window.criteo_q.push(local_criteo_array);
    }
